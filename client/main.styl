body
  font-family Geneva, Arial, Helvetica, sans-serif
a 
td
  cursor pointer
h3
  display inline-block
.navbar-brand > span
  display block
  float left
  position relative
  margin-right 5px
  top -4px
  font-size 1.6em
  
#map
  width 100%
  height 400px
  background-color #CCC
  margin 10px 0

.selected
  color #fff
  &.query > td
    width 40px
    background-color #386DF6
  &.venue > td
    background-color #3B8D04
